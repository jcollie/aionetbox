pipeline {
  agent {
	label 'python37'
  }
  stages {
    // Clean up the workspace
    stage('Clean') {
      steps {
        container('python37-agent'){
          sh 'make clean'
        }
      }
    }

    // Lint the source code.
    stage('Test') {
      steps {
        container('python37-agent'){
          sh 'make test'
        }
      }
    }

  }
}

